\mode<presentation>

\definecolor{craneorange}{RGB}{252,187,6}
\definecolor{craneblue}{RGB}{4,6,76}

\setbeamercolor{structure}{fg=craneblue}

\setbeamercolor{palette primary}{fg=craneblue,bg=craneorange!70}
\setbeamercolor{palette secondary}{fg=craneblue,bg=craneorange!80}
\setbeamercolor{palette ternary}{fg=craneblue,bg=craneorange!90}
\setbeamercolor{palette fourth}{fg=craneblue,bg=craneorange}

\setbeamercolor{titlelike}{parent=palette fourth}

\setbeamercolor{block title}{fg=craneblue,bg=craneorange}
\setbeamercolor{block title alerted}{use=alerted text,fg=black,bg=alerted text.fg!50!bg}
\setbeamercolor{block title example}{use=example text,fg=black,bg=example text.fg!50!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}


\setbeamercolor{sidebar}{bg=craneorange!70}
  
\setbeamercolor{palette sidebar primary}{fg=craneblue}
\setbeamercolor{palette sidebar secondary}{fg=craneblue!75}
\setbeamercolor{palette sidebar ternary}{fg=craneblue!75}
\setbeamercolor{palette sidebar fourth}{fg=craneblue}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>

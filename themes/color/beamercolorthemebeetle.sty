\mode<presentation>

\setbeamercolor{structure}{fg=black!70,bg=black!50}

\setbeamercolor{alerted text}{fg=red!85!black}

\setbeamercolor{item projected}{use=item,fg=white,bg=item.fg!75!black!75}

\setbeamercolor*{palette primary}{use=structure,fg=black!5,bg=structure.bg}
\setbeamercolor*{palette secondary}{use=structure,fg=black!10,bg=structure.bg!95}
\setbeamercolor*{palette ternary}{use=structure,fg=black!15,bg=structure.bg!90}
\setbeamercolor*{palette fourth}{use=structure,fg=black!20,bg=structure.bg!85}

\setbeamercolor*{sidebar}{parent=palette primary}

\setbeamercolor*{palette sidebar primary}{fg=white}
\setbeamercolor*{palette sidebar secondary}{fg=black}
\setbeamercolor*{palette sidebar ternary}{fg=white}
\setbeamercolor*{palette sidebar fourth}{fg=black}

\setbeamercolor*{frametitle}{parent=palette primary,fg=white}
\setbeamercolor*{framesubtitle}{parent=frametitle,fg=black}

\setbeamercolor*{title}{parent=palette primary,fg=white}
\setbeamercolor*{subtitle}{parent=frametitle,fg=black}

\setbeamercolor*{block title}{parent=frametitle}
\setbeamercolor*{block title alerted}{use=alerted text,fg=white,bg=red!50!black!50}
\setbeamercolor*{block title example}{use=example text,fg=white,bg=green!50!black!50}

\setbeamercolor*{block body}{use=block title,fg=black,bg=block title.bg}
\setbeamercolor*{block body alerted}{use=block title alerted,fg=black,bg=block title alerted.bg}
\setbeamercolor*{block body example}{use=block title example,fg=black,bg=block title example.bg}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>

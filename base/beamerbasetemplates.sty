%
% Title and part page templates
%

\newcommand{\beamertemplatedefaulttitlepage}{
  \usetitlepagetemplate{
    \vbox{}
    \vfill
    \begin{centering}
      {\usebeamerfont{title}\structure{\inserttitle}\par}%
      \ifx\insertsubtitle\@empty%
      \else%
        \vskip0.25em%
        {\usebeamerfont{subtitle}\structure{\insertsubtitle}\par}%
      \fi%
      \vskip1em\par
      {\usebeamerfont{author}\insertauthor\par}\vskip1em
      {\usebeamerfont{institute}\insertinstitute\par}\vskip1em
      {\usebeamerfont{date}\insertdate\par}\vskip1.5em
      \inserttitlegraphic\par
    \end{centering}
    \vfill
  }
}
\@onlypreamble\beamertemplatedefaulttitlepage

\newcommand{\beamertemplatedefaultpartpage}{
  \usepartpagetemplate{
    \begin{centering}
      \usebeamerfont{partpage}%
      \structure{\partname~\insertromanpartnumber}
      \vskip1em\par
      {\usebeamerfont{partname}\insertpart\par}
    \end{centering}
  }
} 
\@onlypreamble\beamertemplatedefaultpartpage



%
% Frame title templates
%

\newcommand{\beamertemplatedefaultframetitle}[1][left]{%
  \useframetitletemplate{%
    \vskip0.25em%
    \begin{beamer@fte#1}%
      {\usebeamerfont{frametitle}\structure{\insertframetitle}\par}%
      {%
        \ifx\insertsubtitle\@empty%
        \else%
        {\usebeamerfont{framesubtitle}\structure{\insertframesubtitle}\par}%
        \fi
      }%
    \end{beamer@fte#1}%
  }
}
\@onlypreamble\beamertemplatedefaultframetitle

\newenvironment{beamer@ftecenter}{\begin{centering}}{\end{centering}}
\newenvironment{beamer@fteleft}{%
  \leftskip-\beamer@leftmargin\advance\leftskip by0.3cm%
  \rightskip-\beamer@rightmargin plus1fil\advance\rightskip by0.3cm\leavevmode}{}
\newenvironment{beamer@fteright}{%
  \vskip0.5cm\leftskip1cm plus1fill%
  \rightskip1cm\leavevmode}{}




\newcommand{\beamertemplatedefaulttoc}{
  \usetemplatetocsection[20!parent.bg]{\inserttocsection\par}
  \usetemplatetocsubsection[20!parent.bg]{\leavevmode\leftskip=1.5em\inserttocsubsection\par}
}
\@onlypreamble\beamertemplatedefaulttoc

\newcommand{\beamertemplatenumberedsubsectiontoc}{
  \usetemplatetocsection[20!parent.bg]{\leavevmode\leftskip=2em\inserttocsection}
  \usetemplatetocsubsection[20!parent.bg]
    {\leavevmode\leftskip=2em\rlap{\hskip-2em\inserttocsectionnumber.\inserttocsubsectionnumber}\inserttocsubsection\par}
}
\@onlypreamble\beamertemplatenumberedsubsectiontoc  

\newcommand{\beamertemplatenumberedsectiontoc}{
  \usetemplatetocsection[20!parent.bg]{\leavevmode\inserttocsectionnumber. \inserttocsection\par}
  \usetemplatetocsubsection[20!parent.bg]{\leavevmode\leftskip=2.5em\inserttocsubsection\par}
}
\@onlypreamble\beamertemplatenumberedsectiontoc

\newcommand{\beamertemplatenumberedcirclesectiontoc}{
  \usetemplatetocsection[20!parent.bg]
  {\leavevmode\leftskip=2ex%
    \llap{%
      \usebeamerfont*{section number projected}%
      \usebeamercolor{section number projected}%
      \begin{pgfpicture}{-1ex}{0ex}{1ex}{2ex}
        \color{bg}
        \pgfcircle[fill]{\pgfpoint{0pt}{.75\pgfex}}{1.2\pgfex}
        \pgfbox[center,base]{\color{fg}\inserttocsectionnumber}
      \end{pgfpicture}\kern1.25ex%
    }%
    \inserttocsection\par}  
  \usetemplatetocsubsection[20!parent.bg]
  {\leavevmode\leftskip=2em\inserttocsubsection\par}
}
\@onlypreamble\beamertemplatenumberedcirclesectiontoc

\newcommand{\beamertemplatenumberedsquaresectiontoc}{
  \usetemplatetocsection[20!parent.bg]
  {\leavevmode\leftskip=1.75ex%
    \llap{%
      \usebeamerfont*{section number projected}%
      \usebeamercolor[bg]{section number projected}%
      \vrule width2.25ex height1.85ex depth.4ex%
      \hskip-2.25ex%
      \hbox to2.25ex{\hfil\color{fg}\inserttocsectionnumber\hfil}}%
    \kern1.25ex\inserttocsection\par}
  \usetemplatetocsubsection[20!parent.bg]
  {\leavevmode\leftskip=5ex\llap{%
      \usebeamercolor[bg]{subsection number projected}%
      \vrule width1ex height1ex\kern1ex}\inserttocsubsection\par}
}
\@onlypreamble\beamertemplatenumberedsquaresectiontoc

\newcommand{\beamertemplatenumberedballsectiontoc}{
  \setbeamerfont{section number projected}{size=\scriptsize}

  \usetemplatetocsection[20!parent.bg]
  {\leavevmode\leftskip=2.75ex%
    \llap{%
      \colorlet{parent.bg}{bg}%
      \usebeamercolor{section number projected}%
      \begin{pgfpicture}{-1ex}{-0.7ex}{1ex}{1ex}
        \pgfbox[center,center]{\normalfont\pgfuseshading{tocsphere}}
        \pgfbox[center,center]{\color{fg!90!bg}%
          \usebeamerfont*{section number projected}%
          \inserttocsectionnumber}
      \end{pgfpicture}%
      \kern1.25ex}%
    \inserttocsection\par}
  
  \usetemplatetocsubsection[20!parent.bg]
  {%
    \leavevmode\leftskip=5ex%
    \llap{\raise0.1ex\beamer@usesphere{subsection number projected}{bigsphere}\kern1ex}%
    \inserttocsubsection\par%
  }
}

\newcommand{\beamertemplateballtoc}{
  \usetemplatetocsection[20!parent.bg]
  {\leavevmode\raise0.1ex\beamer@usesphere{section number projected}{bigsphere}\kern1.25ex\inserttocsection\par}

  \usetemplatetocsubsection[20!parent.bg]
  {\leavevmode\leftskip=1.5em\raise0.2ex\beamer@usesphere{subsection number projected}{smallsphere}%
    \kern1ex\inserttocsubsection\par}
}



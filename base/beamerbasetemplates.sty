\newcommand{\beamertemplatedotitem}{%
  \useitemizeitemtemplate{\small\raise0.5pt\hbox{\textbullet}}
  \usesubitemizeitemtemplate{\footnotesize\raise0.5pt\hbox{\textbullet}}
  \usesubsubitemizeitemtemplate{\footnotesize\raise0.5pt\hbox{\textbullet}}
  }

\newcommand{\beamertemplateballitem}{%
  \useitemizeitemtemplate{\raise0.2pt\beamer@usesphere{item projected}{bigsphere}}
  \usesubitemizeitemtemplate{\raise0.2pt\beamer@usesphere{subitem projected}{smallsphere}}%
  \usesubsubitemizeitemtemplate{\raise0.2pt\beamer@usesphere{subsubitem projected}{smallsphere}}%

  \useenumerateitemtemplate{
    \begin{pgfpicture}{-1ex}{-0.65ex}{1ex}{1ex}
      \colorlet{parent.bg}{bg}
      \usebeamercolor{item projected}
      \begin{pgfmagnify}{1.75}{1.75}
        \pgfbox[center,center]{\normalsize\pgfuseshading{bigsphere}}
      \end{pgfmagnify}
      \pgfputat{\pgfpoint{0pt}{0.5pt}}
      {%
        \pgfbox[center,center]{%
          \color{fg}%
          \usebeamerfont*{item projected}%
          \insertenumlabel}}
    \end{pgfpicture}}
  \usesubenumerateitemtemplate{%
    \begin{pgfpicture}{-1ex}{-0.55ex}{1ex}{1ex}
      \colorlet{parent.bg}{bg}
      \usebeamercolor{subitem projected}
      \begin{pgfmagnify}{1.4}{1.4}
        \pgfbox[center,center]{\normalsize\pgfuseshading{bigsphere}}
      \end{pgfmagnify}
      \pgfbox[center,center]{%
          \color{fg}%
          \usebeamerfont*{item projected}%
          \insertsubenumlabel}
    \end{pgfpicture}}
  \usesubsubenumerateitemtemplate{%
    \begin{pgfpicture}{-1ex}{-0.55ex}{1ex}{1ex}
      \colorlet{parent.bg}{bg}
      \usebeamercolor{subsubitem projected}
      \begin{pgfmagnify}{1.4}{1.4}
        \pgfbox[center,center]{\normalsize\pgfuseshading{bigsphere}}
      \end{pgfmagnify}
      \pgfbox[center,center]{%
          \color{fg}%
          \usebeamerfont*{item projected}%
          \insertsubsubenumlabel}
    \end{pgfpicture}}
  \useenumerateitemminitemplate{%
    \begin{pgfpicture}{-1ex}{-0.65ex}{1ex}{1ex}
      \colorlet{parent.bg}{bg}
      \usebeamercolor{item projected}
      \begin{pgfmagnify}{1.75}{1.75}
        \pgfbox[center,center]{\normalsize\pgfuseshading{bigsphere}}
      \end{pgfmagnify}
      \pgfputat{\pgfpoint{0pt}{0.5pt}}
        {\pgfbox[center,center]{%
          \color{fg}%
          \usebeamerfont*{item projected}%
          \insertenumlabel}}
    \end{pgfpicture}}
}
\@onlypreamble\beamertemplateballitem


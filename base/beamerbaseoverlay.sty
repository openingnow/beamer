\def\onslide<#1>{%
  \beamer@endaction%
  \alt<#1>{%
    \ifvoid\beamer@animshowbox\else\ClassError{beamer}{Overprints may not overlap}{}\fi%
    \def\beamer@endaction{%
    \end{minipage}%
    \egroup%

  \pgfdeclareradialshading{bigsphereshaded}{\pgfpoint{-0.1849315ex}{.2260273ex}}%
  {color(0cm)=(bmb@up#1!15);
    color(0.1643835ex)=(bmb@up#1!75);
    color(0.3287671ex)=(bmb@up#1!70!black);
    color(0.4520547ex)=(bmb@up#1!50!black);

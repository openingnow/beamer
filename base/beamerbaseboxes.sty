    color(0bp)=(black!50!averagebackgroundcolor);
    color(4bp)=(averagebackgroundcolor)}
  \pgfdeclareradialshading{bmb@shadowballlarge}{\pgforigin}{%
    color(0bp)=(black!50!averagebackgroundcolor);
    color(4bp)=(black!50!averagebackgroundcolor);
    color(8bp)=(averagebackgroundcolor)}
